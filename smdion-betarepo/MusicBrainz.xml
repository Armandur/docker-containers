<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>MusicBrainz</Name>
  <Description>This is a Dockerfile setup for MusicBrainz. &#13;
Edit /config/dDBDefs.pm[br][br]&#13;
[b][span style=\'color: #E80000;\']Directions:[/span][/b][br]&#13;
[b]/config[/b] : this path is used to store the configuration MusicBrainz.[br]&#13;
</Description>
  <Registry>https://registry.hub.docker.com/u/smdion/docker-ddclient/</Registry>
  <Repository>smdion/docker-ddclient</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Environment/>
  <Networking>
    <Mode>host</Mode>
    <Publish>
      <Port>
        <HostPort>5000</HostPort>
        <ContainerPort>5000</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    <Publish/>
  </Networking>
  <Data>
    <Volume>
      <HostDir></HostDir>
      <ContainerDir>/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Version></Version>
  <WebUI></WebUI>
  <Banner></Banner>
  <Icon></Icon>
</Container>