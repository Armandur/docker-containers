<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>MusicBrainz</Name>
  <Description>
    MusicBrainz is an open music encyclopedia that collects music metadata and makes it available to the public.[br][br]
    [b][span style='color: #E80000;']Directions:[/span][/b][br]
    [b]/config[/b] : this path is used to store the configuration and the SSL certs files of the reverse proxy.[br]
  </Description>
  <Registry>https://registry.hub.docker.com/u/smdion/musicbrainz-docker/</Registry>
  <Repository>smdion/musicbrainz-docker</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>5000</HostPort>
        <ContainerPort>5000</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Environment>
    <Variable>
      <Name></Name>
      <Value></Value>
    </Variable>
  </Environment>
  <Data>
    <Volume>
      <HostDir></HostDir>
      <ContainerDir>/media/dbdump</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/etc/localtime</HostDir>
      <ContainerDir>/etc/localtime</ContainerDir>
      <Mode>ro</Mode>
    </Volume>
  </Data>
</Container>
