<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>nginxReverseProxy</Name>
  <Description>
  </Description>
  <Registry>https://registry.hub.docker.com/u/smdion/docker-nginx/</Registry>
  <Repository>smdion/docker-nginx</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Networking>
    <Mode>host</Mode>
    <Publish>
      <Port>
        <HostPort>80</HostPort>
        <ContainerPort>80</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>443</HostPort>
        <ContainerPort>443</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Environment>
    <Variable>
      <Name></Name>
      <Value></Value>
    </Variable>
  </Environment>
  <Data>
    <Volume>
      <HostDir></HostDir>
      <ContainerDir>/var/log/nginx</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
        <Volume>
      <HostDir></HostDir>
      <ContainerDir>/etc/nginx/certs</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir></HostDir>
      <ContainerDir>/etc/nginx/sites-enabled</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
</Container>
