<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>Keybox</Name>
  <Description>
    This is a Dockerfile for "Keybox" - (http://sshkeybox.com/)[br][br]
    [b][span style='color: #E80000;']Directions:[/span][/b][br]
  </Description>
  <Registry>https://registry.hub.docker.com/u/smdion/docker-keybox/</Registry>
  <Repository>smdion/docker-keybox</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>8443</HostPort>
        <ContainerPort>8443</ContainerPort>
        <Protocol></Protocol>
      </Port>
    </Publish>
  </Networking>
  <Environment>
    <Variable>
      <Name></Name>
      <Value></Value>
    </Variable>
  </Environment>
  <Data>
    <Volume>
      <HostDir>/etc/localtime</HostDir>
      <ContainerDir>/etc/localtime</ContainerDir>
      <Mode>ro</Mode>
    </Volume>
  </Data>
  <Icon></Icon>
  <Banner></Banner>
</Container>
